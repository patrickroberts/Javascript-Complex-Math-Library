{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint:latest",
    "tslint-config-airbnb",
    "tslint-eslint-rules",
    "tslint-config-prettier"
  ],
  "jsRules": {},
  "rules": {
    "array-type": [true, "array"],
    "no-bitwise": false,
    "no-shadowed-variable": [
      true,
      {
        "import": false
      }
    ],
    "variable-name": {
      "options": [
        "allow-leading-underscore",
        "allow-pascal-case"
      ]
    }
  },
  "rulesDirectory": [
    "node_modules/tslint-eslint-rules/dist/rules"
  ]
}
